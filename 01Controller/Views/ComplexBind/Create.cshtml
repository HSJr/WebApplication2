
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>產品資料新增作業</title>
</head>
<body>
    <h2>產品資料新增作業</h2>
    <form action="/ComplexBind/Create" method="post">
        <p>品號：<input id="Pid" name="Pid" type="text"></p>
        <p>品名：<input id="PName" name="PName" type="text"></p>
        <p>單價：<input id="Price" name="Price" type="text"></p>
        <input id="Submit1" type="submit" value="新增商品" />
    </form>
    <hr />
    <h3>新增作業完成</h3>
    <p>內容如下</p>
    <p>品號：@ViewBag.Pid</p>
    <p>品名：@ViewBag.PName</p>
    <p>單價：@ViewBag.Price</p>
</body>
</html>